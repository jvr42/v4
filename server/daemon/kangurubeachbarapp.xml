<service>
	<id>kangurubeachbarapp.exe</id>
	<name>KANGURU BEACH BAR APP</name>
	<description>Sistema de Ventas de Kanguru Beach Bar</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>C:\Apps\2016\services\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\Apps\2016\kanguru\server\app.js</argument>
	<argument>--log</argument>
	<argument>KANGURU BEACH BAR APP wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<stoptimeout>30sec</stoptimeout>
	<workingdirectory>C:\Apps\2016\services</workingdirectory>
</service>